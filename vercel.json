{
    "version": 2,
    "builds": [
        {
            "src": "/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "routes": [
        {
            "src": "^/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)\\.png",
            "dest": "/$1.png"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}